pipeline {
    agent { label 'linux' }
    tools {
        maven 'apache-maven-3.0.5'
    }
    stages {
        stage('checkout') {
            steps {
                git 'https://github.com/elenaandries/weather.git'
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean compile'       
            }
        }
    }
}
